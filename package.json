{
  "name": "exilibrium",
  "productName": "Exilibrium",
  "version": "1.0.0-beta.7",
  "description": "Exilibrium is an ExchangeCoin full node HD wallet GUI",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "./node_modules/.bin/eslint app scripts && ./node_modules/.bin/stylelint app/style/*.less",
    "lint-fix": "./node_modules/.bin/eslint app scripts --fix && ./node_modules/.bin/stylelint app/style/*.less --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=4096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/ --debug --customBinPath=./bin",
    "dev-main": "cross-env HOT=1 NODE_ENV=development electron --inspect=0.0.0.0:9229 -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"electron-builder install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev-renderer": "node -r babel-register ./node_modules/.bin/webpack-dev-server --port 3000 --config ./webpack.config.development.js",
    "dev": "npm run dev-renderer & npm run dev-main",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64 --ia32",
    "package-linux": "npm run build && build --linux",
    "package-mac": "npm run build && build --mac",
    "package-all": "npm run build && build -mwl",
    "package-dev-linux": "npm run build && build --linux tar.gz",
    "cleanup": "mop -v",
    "i18n-prepare-untranslated": "node ./scripts/prepareUntranslated.js && ./node_modules/.bin/rip json2pot 'app/i18n/extracted/**/*.json' -c id -o app/i18n/pot/exilibrium.pot",
    "i18n-assemble-translated": "node ./scripts/assembleTranslated.js && ./node_modules/.bin/rip po2json 'app/i18n/po/*.po' -m 'app/i18n/extracted/**/*.json' -c id -o app/i18n/translations"
  },
  "jest": {
    "modulePaths": [
      "app"
    ],
    "moduleNameMapper": {
      "grpc": "<rootDir>/test/mock/grpc.js"
    }
  },
  "build": {
    "appId": "com.Electron.Exilibrium",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "staticPages/",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "bin",
        "to": "bin"
      },
      "*.node"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "public.app-category.tools",
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "directories": {
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EXCCoin/exilibrium.git"
  },
  "author": {
    "name": "The ExchangeCoin Developers",
    "email": "support@excc.co",
    "url": "https://github.com/EXCCoin"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EXCCoin/exilibrium/issues"
  },
  "keywords": [
    "exccoin",
    "excc",
    "exilibrium",
    "electron",
    "react",
    "react-router",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/EXCCoin/exilibrium#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^1.0.0",
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "grpc": "^1.15.1",
    "ini": "^1.3.5",
    "is-running": "^2.1.0",
    "lodash": "^4.17.11",
    "material-ui": "^0.19.4",
    "prop-types": "^15.6.2",
    "qr-image": "^3.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-infinite-scroller": "^1.2.1",
    "react-intl": "^2.6.0",
    "react-intl-po": "^2.2.2",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-router-transition": "^1.2.1",
    "react-select": "1.0.0-rc.10",
    "react-timeout": "^1.1.2",
    "recharts": "^1.2.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "sjcl": "^1.0.7",
    "source-map-support": "^0.5.9",
    "string-argv": "^0.1.1",
    "systeminformation": "^3.45.6",
    "taskkill": "^2.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-tcomb": "^0.3.21",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "boiler-room-custodian": "^0.4.2",
    "chai": "^3.5.0",
    "chalk": "^2.4.1",
    "clipboard-copy": "^1.2.0",
    "concurrently": "^3.1.0",
    "core-decorators": "^0.19.0",
    "cross-env": "^3.1.3",
    "dateformat": "^2.0.0",
    "devtron": "^1.4.0",
    "enzyme": "^3.1.0",
    "eslint": "^5.6.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^1.1.11",
    "google-protobuf": "^3.1.1",
    "grpc-tools": "^1.6.6",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "less": "^3.8.1",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "node-gyp": "^3.6.2",
    "node-loader": "^0.6.0",
    "prettier": "^1.14.3",
    "react-intl-translations-manager": "^5.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "sinon": "^1.17.6",
    "spectron": "^3.4.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "tcomb": "^3.2.15",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^4.1.4"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
